#include <iostream>
using namespace std;
int main(){
    int t;
    cin>>t;
    int n;
    for(int j=0; j<t; j++){
        int n;
        cin>>n;
        int subarr [n];
        int x;
        cin>>subarr[0];

        for (int i=1; i<n; i++){
            cin>>x;
            subarr[i]=subarr[i-1]+x;
        }


    }
}